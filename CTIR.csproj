<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <LangVersion>latest</LangVersion>
    <RootNamespace>ObjectIR.ContractFrontend</RootNamespace>
    <AssemblyName>CTIR</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>ctir</ToolCommandName>
    <PackageId>ctir</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../ObjectIR.Core/ObjectIR.Core.csproj" />
  </ItemGroup>
</Project>
